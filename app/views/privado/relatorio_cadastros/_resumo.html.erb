<div class="resumo relatorio_cadastros">

<div class="section_header">
	<%= link_to_function(image_tag('botoes/printer2.png'), "$('iframe#para_impressao').attr('src', '#{imprimir_relatorio_cadastros_path(filtros: params[:filtros])}')", style:'float: left;') %>
	<h2><%= t('.titulo') %></h2>
</div>
<br>
<table>
  <thead>
    <tr>
      <strong><th align="left">Nome</th></strong>
      <th align="left">Data Cadastro</th>
    </tr>
  </thead>
  <tbody>
    <% @pacientes.each do |paciente| %>
      <tr>
        <td><%= paciente.nome %></td>
        <td><%= l(paciente.created_at.to_date) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>